ALGORITHM 
VAR
    set_declarations
BEGIN
    FUNCTION insertionSort(int[] arr, int n)
{
    int i, key, j;
    FOR i FROM 0 TO n - 1 STEP 1 DO
    {
        key = arr[i];
        j = i - 1;
        while (j >= 0 && arr[j] > key)
        {
            arr[j + 1] = arr[j];
            j = j - 1;
        }
        arr[j + 1] = key;
    }
    return arr;
}


END

